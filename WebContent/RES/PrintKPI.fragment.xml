<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.commons.layout" class="sapUiSizeCompact">

	<Dialog xmlns="sap.m" id="idPrintKPI" modal="true" stretch="true" resizable="true"
		 title="{i18n>Subtotals}">
		<HBox>
			<VBox id="idService" class="sapUiSmallMarginleft" width="50%">
				<items>
					<Title text="Service" />
					<Panel headerText="Количество проходов автомобилей"
						expandable="true" expanded="true">
						<HBox>
							<Text text="8.1. Для клиента" width="30%" />
							<Text text="Day" />
							<Text text="Night" />
							<Text text="Day" />
							<Text text="Night" />
						</HBox>
					</Panel>


				</items>
			</VBox>

			<VBox id="idSparepars">
				<items>
					<Title text="Spareparts" />
					<Panel headerText="Сервисград, %"
						expandable="true" expanded="true">
						<HBox>
							<Text text="1.1. Для клиента" width="30%" />
							<Text text="Day" />
							<Text text="Night" />
							<Text text="Day" />
							<Text text="Night" />
						</HBox>
					</Panel>

				</items>
			</VBox>

		</HBox>

		<buttons>

			<Button id="btnUploadDldClose" text="{i18n>btnClose}" press="onPrintDlgClose" />
		</buttons>
	</Dialog>

</core:FragmentDefinition>